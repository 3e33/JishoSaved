{
  "manifest_version": 2,
  "name": "JishoSaved",
  "version": "1.0",

  "description": "Improves the experience when using Jisho.",

  "permissions": ["storage"],

  "icons": {
    "48": "icons/icon-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.jisho.org/*"],
      "js": ["JishoSaved.js"]
    }
  ]
}
